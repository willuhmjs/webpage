		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"rehype-code-titles": "^1.2.0",
		"rehype-pretty-code": "^0.9.4",
		"shiki": "^0.14.1",
		"svelte": "^3.54.0",
